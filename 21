//21
#include <stdio.h>

int main()
{
char sym;
scanf("%c", sym);
    switch(sym)
    {
        case 'a':
        case 'A':
        case 'o':
        case 'O':
        case 'i':
        case 'I':
        case 'u':
        case 'U':
        case 'e':
        case 'E':
        printf("dzaynavor");
        break;

        case 0 ... 64: 
        case 91 ... 96:
        case 123 ... 127:
        break;

        default:
        printf("baxadzayn");

    }




    return 0;
}
