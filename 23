//23
#include <stdio.h>

int main()
{
int a, b, c, d;
scanf(" %d", &a);
scanf(" %d", &b);
scanf(" %d", &c);
scanf(" %d", &d);
if(a + b + c + d ==0){
if(a <= b && a <= c && a <= d)
printf("%d", a);
if(b <= a && b <= c && b <= d)
printf("%d", b);
if(c <= b && c <= a && c <= d)
printf("%d", c);
}
else
printf("%d", d);
    return 0;
}
